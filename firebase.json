{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "app",
    "rewrites": [{
      "source": "/public/**",
      "destination": "/public.html"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }],
    "redirects": [{
        "source": "/sharing-resources.firebaseapp.com/*",
        "destination": "https://sharing-resources.firebaseapp.com",
        "type": 302
    }]
  }
}
